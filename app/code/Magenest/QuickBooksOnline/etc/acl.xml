<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_QuickBooksOnline::integration" title="QuickBooks Online" sortOrder="60">
                    <resource id="Magenest_QuickBooksOnline::toquickbook" title="Magento Sync To QuickBooks Online" sortOrder="10">
                        <resource id="Magenest_QuickBooksOnline::payment_methods" title="Payment Methods Mapping" sortOrder="10"/>
                        <resource id="Magenest_QuickBooksOnline::tax" title="Tax Code Mapping" sortOrder="20"/>
                        <resource id="Magenest_QuickBooksOnline::queue" title="List Queue" sortOrder="30"/>
                        <resource id="Magenest_QuickBooksOnline::logs" title="View History Logs" sortOrder="40"/>
                        <resource id="Magenest_QuickBooksOnline::config_qbonline" title="Configuration" sortOrder="50"/>
                    </resource>
                    <resource id="Magenest_QuickBooksOnline::tomagento" title="QuickBooks Online Sync To Magento" sortOrder="20">
                        <resource id="Magenest_QuickBooksOnline::update" title="Update" sortOrder="10">
                            <resource id="Magenest_QuickBooksOnline::update_customer" title="Customer" sortOrder="10"/>
                            <resource id="Magenest_QuickBooksOnline::update_product" title="Product" sortOrder="20"/>
                        </resource>
                        <resource id="Magenest_QuickBooksOnline::create_new" title="Create New" sortOrder="20">
                            <resource id="Magenest_QuickBooksOnline::create_customer" title="Customer" sortOrder="10"/>
                            <resource id="Magenest_QuickBooksOnline::create_product" title="Product" sortOrder="20"/>
                            <resource id="Magenest_QuickBooksOnline::create_salesreceipt" title="Sales Receipts" sortOrder="30"/>
                        </resource>
                    </resource>
                </resource>

            </resource>
        </resources>
    </acl>
</config>
